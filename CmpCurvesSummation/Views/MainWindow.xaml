<Window x:Class="CmpCurvesSummation.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views7="clr-namespace:CmpCurvesSummation.Views"
        xmlns:viewModels="clr-namespace:CmpCurvesSummation.ViewModels"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="900" Width="1400">
    <Control.DataContext>
        <viewModels:MainWindowViewModel/>
    </Control.DataContext>
    <Grid>
        <views7:MainCtrl x:Name="mainCtrl"></views7:MainCtrl>
    </Grid>
</Window>



